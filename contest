int32_t main()
{
	int i,n;
	cin >> n;
	for(int i=1; i<=n; i++)
	{
		cin >> a[i];
		pf[i]=pf[i-1]+a[i];
	}
	int t;
	cin >> t;
	while(t--)
	{
       int l,r;
	   cin >> l>> r;
